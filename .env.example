# ======================================================================
# POLLINATIONS MVP - IMAGE GENERATION API
# ======================================================================

# Server Configuration
PORT=16384

# External Image Generation Service
# You can use any compatible API endpoint that generates images from prompts
# Default: Use Pollinations.AI public API (free, no key required)
IMAGE_API_URL=https://image.pollinations.ai/prompt

# Alternative: Use your own FLUX/Stable Diffusion API
# IMAGE_API_URL=https://your-api-endpoint.com/generate
# IMAGE_API_KEY=your-api-key-here

# Rate Limiting (milliseconds between requests per IP)
RATE_LIMIT_INTERVAL=30000

# Cache Directory
CACHE_DIR=./cache
